{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "startServerCommand": "cd apps/web && pnpm start",
      "url": [
        "http://localhost:3000"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.7
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.8
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.8
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}