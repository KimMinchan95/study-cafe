# Environment
NODE_ENV=development

# Server
PORT=3001

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/study_cafe

# Redis (세션 저장소)
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=  # 비밀번호 사용 시 설정

# Session (쿠키 서명용, 프로덕션에서는 반드시 변경)
SESSION_SECRET=dev-session-secret-change-in-production

# CORS
CORS_ORIGIN=http://localhost:3000

# Password
BCRYPT_SALT_ROUNDS=10